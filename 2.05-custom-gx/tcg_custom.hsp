*card_action_choose_target
	if ( cdbit(TCG_BIT_SILENCED, effac@tcg) ) {
		return
	}
	makecardlist
	target@tcg = (-1)
	target_num@tcg = 1
	activate_anywhere@tcg = 1
	// activate_anywhere@tcg = 0
	// if ( effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_DEATHRATTLE ) { activate_anywhere@tcg = 1 }
	// if ( effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_SACRIFICE ) { activate_anywhere@tcg = 1 }
	// if ( effref@tcg(TCG_SKILL_TYPE, eff@tcg) == TCG_SKILL_TYPE_ONKILL ) { activate_anywhere@tcg = 1 }
	if ( effref@tcg(TCG_SKILL_TARGET, eff@tcg) == TCG_SKILL_TARGET_PLAYER ) { target@tcg = cp@tcg }
	if ( effref@tcg(TCG_SKILL_TARGET, eff@tcg) == TCG_SKILL_TARGET_OPPONENT ) { target@tcg = tp@tcg }
	if ( effref@tcg(TCG_SKILL_TARGET, eff@tcg) == TCG_SKILL_TARGET_SELF ) { target@tcg = effac@tcg }
	if ( effref@tcg(TCG_SKILL_TARGET, eff@tcg) == TCG_SKILL_TARGET_ALL_FIELD_ALL ) { target_num@tcg = 14 }
	// dialog "" + cc@tcg + "/" + effref@tcg(TCG_SKILL_EFF, eff@tcg) + "/" + target@tcg
	repeat target_num@tcg
		cnt2@tcg = cnt
		if ( effref@tcg(TCG_SKILL_TARGET, eff@tcg) / 10 == 5 ) {
			if ( cnt \ 7 >= clistmax@tcg(1+cnt/7) ) {
				continue
			}
			target@tcg = clist@tcg(cnt \ 7, 1+cnt/7)
		} 
		repeat limit(effref@tcg(TCG_SKILL_REPEAT, eff@tcg), 1, 10)
			cardstack@tcg(TCG_STACK, stack@tcg) = effac@tcg, effref@tcg(TCG_SKILL_EFF, eff@tcg), cp@tcg, activate_anywhere@tcg, target@tcg, effref@tcg(TCG_SKILL_POWER, eff@tcg)
			stack@tcg++
		loop
	loop
	actionproc
	return

*card_loadplayerdeck
	cardsize@tcg = 0
	file = "" + exedir + "tmp\\deck_" + curdeck + ".s2"
	existwrapper file
	if ( rtval != (-1) ) { 
		fmode = 24
		gosub *game_ctrlFile@
		repeat MAX_CARD
			cnt2@tcg = cnt
			if ( deck(cnt2@tcg) <= 0 ) {
				continue
			}
			repeat deck(cnt2@tcg)
				if ( cardsize@tcg > 30 ) {
					break
				}
				card_ref cnt2@tcg
				if ( stat >= 0 ) {
					create_card cardsize@tcg, stat
					card@tcg(TCG_CARD_LOCATION, cardsize@tcg) = TCG_LOCATION_DECK, rp@tcg
					cardsize@tcg++
				}
			loop
		loop
	}
	return

*card_generateopponentdeck
	cardsize@tcg = 0
	dim deck_opponent, MAX_CARD
	card_ref cdata(CDATA_ID, tc)
	deckuserdomain@tcg = cardrefdomain + 1
	if ( cardrefdomain == 5 | cardrefdomain == 6 ) {
		deckuserdomain@tcg = rnd(6) + 1
		if ( deckuserdomain@tcg >= 5 ) {
			deckuserdomain@tcg += 2
		}
	} 
	repeat 30
		cc@tcg = 40 + cnt
		rndlvl@tcg = cdata(CDATA_LEVEL, tc) + 80
		targetdeck@tcg = 2 
		if ( rnd(3) > 0 ) { 
			rndfixlvl@tcg = 2
			rndrace@tcg = cdatan(CDATAN_RACE, tc) 
		} else {
			if ( rnd(5) > 0 ) {
				rndfixlvl@tcg = 2
				rnddomain@tcg = deckuserdomain@tcg
			} else {
				if ( rnd(5) != 0 ) {
					rnddomain@tcg = 5
				} else {
					rnddomain@tcg = 6
				}
			}
		}
		gosub *card_filter
		card_ref cardno
		if ( stat == 0 ) {
			gosub *card_filter
		}
		create_card cc@tcg, stat
		deck_opponent(card@tcg(TCG_CARD_REF_DBID), cc@tcg)++
		card@tcg(TCG_CARD_LOCATION, cc@tcg) = TCG_LOCATION_DECK, rp@tcg
		cardsize@tcg++
	loop
	return

*card_pack_custom
	repeat 5
		rndrace@tcg = ""
		rnddomain@tcg = 1 + rnd(6)
		if ( rnddomain@tcg >= 5 ) { rnddomain@tcg += 2 }
		if ( cnt == 4 | ( cnt == 3 & rnd(4) == 0 ) | ( cnt == 2 & rnd(20) == 0 ) ) {
			rnddomain@tcg = 5
		}
		if ( cnt == 4 & rnd(4) == 0 ) {
			rnddomain@tcg = 6
			if ( rnd(50) == 0 ) {
				if ( rnd(1) == 0 ) { cardno = CREATURE_ID_ITZPALT }
				if ( rnd(2) == 0 ) { cardno = CREATURE_ID_OPATOS }
				if ( rnd(3) == 0 ) { cardno = CREATURE_ID_MANI }
				if ( rnd(4) == 0 ) { cardno = CREATURE_ID_KUMIROMI }
				if ( rnd(5) == 0 ) { cardno = CREATURE_ID_EHEKATL }
				if ( rnd(6) == 0 ) { cardno = CREATURE_ID_JURE }
				if ( rnd(7) == 0 ) { cardno = CREATURE_ID_LULWY }
				if ( rnd(8) == 0 ) { cardno = CREATURE_ID_YACATECT }
				if ( rnd(10) == 0 ) {
					txtef COLOR_YELLOW
					txt "You got an ultra rare card!"
					chatc@dp = (-1)
					txtef COLOR_YELLOW
					txt lang("「うみゃ～？たらばがに！」", "\"Mewmewmew? Coconut crab!\"")
					spawnCard CREATURE_ID_EHEKATL
					if ( rnd(1) == 0 ) { cardno = CREATURE_ID_THE_ELEMENT }
					if ( rnd(2) == 0 ) { cardno = CREATURE_ID_BUILDUP_OPATOS }
					if ( rnd(3) == 0 ) { cardno = CREATURE_ID_DEUS_EX_MANINA }
					if ( rnd(4) == 0 ) { cardno = CREATURE_ID_INSANE_KUMIROMI }
					if ( rnd(5) == 0 ) { cardno = CREATURE_ID_GOD_INSIDE_EHEKATL }
					if ( rnd(6) == 0 ) { cardno = CREATURE_ID_BLESSED_JURE }
					if ( rnd(7) == 0 ) { cardno = CREATURE_ID_DEVASTATE_LULWY }
					if ( rnd(8) == 0 ) { cardno = CREATURE_ID_HYPER_YACATECT }
				} else {
					txtef COLOR_YELLOW
					txt "You got a super rare card!"
					chatc@dp = (-1)
					txtef COLOR_YELLOW
					txt lang("「うみゃ～？」", "\"Mew?\"")
				}
				spawnCard cardno
				continue
			}
		}
		if ( rnddomain@tcg == 5 ) {
			txtef COLOR_YELLOW
			txt "You got an uncommon card!"
		}
		if ( rnddomain@tcg == 6 ) {
			txtef COLOR_YELLOW
			txt "You got a rare card!"
		}
		gosub *card_filter
		spawnCard cardno
	loop
	goto *act_open_SWEND1
	return

*card_filter
	repeat 500
		cardno = rnd(MAX_CREATURE_ID - 3) + 1
		if ( gdata(GDATA_FLAG_MAIN) < 200 ) {
			cardno = rnd(347) + 1
			if ( cardno == CREATURE_ID_ZEOME_THE_FALSE_PROPHET ) { continue }
			if ( cardno == CREATURE_ID_ORPHE_THE_CHAOS_CHILD ) { continue }
		}
		if ( gdata(GDATA_FLAG_MAIN) < 350 & gdata(GDATA_FLAG_MAIN) >= 200 ) {
			cardno = rnd(528) + 1
			if ( cardno == CREATURE_ID_MESHERA_ALPHA_THE_DEFORMED_ANGEL ) { continue }
			if ( cardno == CREATURE_ID_MESHERA_PLANT ) { continue }
			if ( cardno == CREATURE_ID_ORPHE_THE_CHAOS_FOLLOWER ) { continue }
		}
		if ( gdata(GDATA_FLAG_MAIN) >= 350 ) {
			cardno = rnd(918) + 1
			if ( gdata(GDATA_FLAG_SUB_REGULUS_ZANAN) < 1003 ) {
				if ( cardno == CREATURE_ID_RENAI_THE_OVER_MESHERA ) { continue }
			}
			if ( gdata(GDATA_FLAG_MAIN) < 540 ) {
				if ( cardno == CREATURE_ID_FASHIONABLE_KUNOICHI ) { continue }
			}
			if ( gdata(GDATA_FLAG_MAIN) < 540 ) {
				if ( cardno <= CREATURE_ID_TEZCATLIPOCA & cardno >= CREATURE_ID_TALON_SOLDIER ) { continue }
			}
		}
		if ( gdata(GDATA_FLAG_MAIN) >= 715 ) {
			cardno = rnd(MAX_CREATURE_ID - 3) + 1
			if ( gdata(GDATA_FLAG_MAIN) < 760 ) {
				if ( cardno == CREATURE_ID_ENTHUMESIS ) { continue }
			}
			if ( gdata(GDATA_FLAG_MAIN) < 760 ) {
				if ( cardno <= CREATURE_ID_MELUGAST_AO_I_THE_DIMENSION_DRIVER & cardno >= CREATURE_ID_THE_CHAOS_CHILD ) { continue }
			}
			if ( gdata(GDATA_FLAG_MAIN) < 950 ) {
				if ( cardno <= CREATURE_ID_OBLIVION_BEAST & cardno >= CREATURE_ID_LOYTER_THE_CRIMSON_OF_DOLE ) { continue }
				if ( cardno <= CREATURE_ID_TONI_THE_ANGLER & cardno >= CREATURE_ID_CARTER_THE_SKILLED_FOOT ) { continue }
				if ( cardno <= CREATURE_ID_STRANGE_OLD_PERSON & cardno >= CREATURE_ID_MANSON_THE_CAREFUL_ADVENTURER ) { continue }
				if ( cardno == CREATURE_ID_BONYAC_THE_GENERAL_VENDOR ) { continue }
				if ( cardno <= CREATURE_ID_YAYAUHQUI_TEZCATLIPOCA & cardno >= CREATURE_ID_MIKRAANESIS ) { continue }
			}
		}
		if ( cardno == CREATURE_ID_USER ) { continue }
		if ( cardno <= CREATURE_ID_ITZPALT & cardno >= CREATURE_ID_HYPER_YACATECT ) { continue }
		if ( cardno <= CREATURE_ID_BLESSED_JURE & cardno >= CREATURE_ID_INSANE_KUMIROMI ) { continue }
		if ( cardno == CREATURE_ID_OPATOS ) { continue }
		if ( cardno == CREATURE_ID_KUMIROMI ) { continue }
		if ( cardno == CREATURE_ID_MANI ) { continue }
		if ( cardno == CREATURE_ID_EHEKATL ) { continue }
		if ( cardno == CREATURE_ID_GOD_INSIDE_EHEKATL ) { continue }
		if ( cardno == CREATURE_ID_LULWY ) { continue }
		card_ref cardno
		if ( stat < 0 ) { continue }
		if ( cnt > 400 ) { break }
		if ( cardrefdomain != (rnddomain@tcg - 1) & rnddomain@tcg != 0 ) { continue }
		if ( cardrefrace != rndrace@tcg & rndrace@tcg != "" ) { continue }
		if ( cardrefclass != rndclass@tcg & rndclass@tcg != "" ) { continue }
		if ( cardrefsex != rndsex@tcg & rndsex@tcg != "" ) { continue }
		if ( cardreflvl > rndlvl@tcg & rndlvl@tcg != 0 ) { continue }
		if ( cardrefquality != rndfixlvl@tcg & rndfixlvl@tcg != 0 ) { continue }
		if ( targetdeck@tcg == 1 ) {
			if ( deck(cardno) >= rnd(rnd(5)+1) ) { continue }
			if ( deck(cardno) >= 1 & cardrefdomain == 5 ) { continue }
		}
		if ( targetdeck@tcg == 2 ) {
			if ( deck_opponent(cardno) >= rnd(rnd(5)+1) ) { continue }
			if ( deck_opponent(cardno) >= 1 & cardrefdomain == 5 ) { continue }
		}
		break
	loop
	targetdeck@tcg = 0
	rndsex@tcg = ""
	rndclass@tcg = ""
	rndrace@tcg = ""
	rnddomain@tcg = 0
	rndlvl@tcg = 0
	rndfixlvl@tcg = 0 // 2 OR 6
	return

